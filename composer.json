{
  "name": "mimmi20/navigation-helper-htmlify",
  "description": "Provides a accept-page helper for Laminas- and Mezzio Navigation",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/navigation-helper-htmlify",
  "support": {
    "issues": "https://github.com/mimmi20/navigation-helper-htmlify/issues",
    "source": "https://github.com/mimmi20/navigation-helper-htmlify"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "ext-mbstring": "*",
    "laminas/laminas-servicemanager": "^3.22.1",
    "laminas/laminas-view": "^2.38.0",
    "mimmi20/laminasviewrenderer-helper-htmlelement": "^3.0.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "laminas/laminas-i18n": "^2.30.0",
    "laminas/laminas-modulemanager": "^2.17.0",
    "laminas/laminas-navigation": "^2.20.0",
    "mimmi20/coding-standard": "^6.0.28",
    "mimmi20/mezzio-navigation": "^4.0.3",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.13",
    "phpstan/phpstan-deprecation-rules": "^2.0.2",
    "phpunit/phpunit": "^12.1.3",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^2.0.14",
    "rector/type-perfect": "^2.0.2",
    "shipmonk/composer-dependency-analyser": "^1.8.2",
    "symfony/console": "^7.2.5",
    "symfony/process": "^7.2.5",
    "symplify/phpstan-rules": "^14.6.9",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-i18n": "Required to use the Translator",
    "laminas/laminas-navigation": "Required to use with Laminas",
    "mimmi20/mezzio-navigation": "Required to use with Mezzio"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\NavigationHelper\\Htmlify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\NavigationHelper\\Htmlify\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\NavigationHelper\\Htmlify",
      "config-provider": "Mimmi20\\NavigationHelper\\Htmlify\\ConfigProvider"
    }
  }
}
