{
    "name": "mimmi20/navigation-helper-htmlify",
    "type": "library",
    "description": "Provides a accept-page helper for Laminas- and Mezzio Navigation",
    "homepage": "https://github.com/mimmi20/navigation-helper-htmlify",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "ext-mbstring": "*",
        "mimmi20/laminasviewrenderer-helper-htmlelement": "^2.0.6"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "infection/infection": "^0.27.9",
        "laminas/laminas-i18n": "^2.26.0",
        "laminas/laminas-modulemanager": "^2.15.0",
        "laminas/laminas-navigation": "^2.19.1",
        "laminas/laminas-servicemanager": "^3.22.1",
        "mimmi20/coding-standard": "^5.2.13",
        "mimmi20/mezzio-navigation": "^3.0.4",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.58",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.5.10",
        "psr/container": "^1.1.2 || ^2.0.2",
        "rector/rector": "^1.0.0"
    },
    "suggest": {
        "ext-intl": "required to use the Translator",
        "laminas/laminas-i18n": "Required to use the Translator",
        "laminas/laminas-navigation": "Required to use with Laminas",
        "laminas/laminas-servicemanager": "Required to use the navigation factories",
        "mimmi20/mezzio-navigation": "Required to use with Mezzio"
    },
    "autoload": {
        "psr-4": {
            "Mimmi20\\NavigationHelper\\Htmlify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mimmi20Test\\NavigationHelper\\Htmlify\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/mimmi20/navigation-helper-htmlify/issues",
        "source": "https://github.com/mimmi20/navigation-helper-htmlify"
    },
    "config": {
        "preferred-install": "dist",
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        }
    },
    "extra": {
        "laminas": {
            "component": "Mimmi20\\NavigationHelper\\Htmlify",
            "config-provider": "Mimmi20\\NavigationHelper\\Htmlify\\ConfigProvider"
        }
    }
}
